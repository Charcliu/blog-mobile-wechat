/* pages/launch/launch.wxss */

.launch {
  width: 200rpx;
  height: 200rpx;
  animation: launchAnimate 3s;
  margin: auto;
}

@keyframes launchAnimate {
  0% {
    transform: scale(1, 1) rotate(0deg) translate(0rpx, 0rpx);
  }

  100% {
    transform: scale(2, 2) rotate(360deg) translate(0rpx, 200rpx);
  }
}
